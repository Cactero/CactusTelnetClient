<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration>
    <servers>
    </servers>
    <playerCommands>
        <playerCommand>
            <name>Ban</name>
            <format>ban $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Ban Name</name>
            <format>banname $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Ban IP</name>
            <format>ban $TARGET_IP</format>
        </playerCommand>
        <playerCommand>
            <name>Toggle Mute</name>
            <format>mute $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Kick</name>
            <format>kick $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Tempban 5m + Reason</name>
            <format>tempban $TARGET_NAME 5m $REASON</format>
        </playerCommand>
        <playerCommand>
            <name>Tempban 5m</name>
            <format>tempban $TARGET_NAME 5m</format>
        </playerCommand>
        <playerCommand>
            <name>Smite</name>
            <format>smite $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Op</name>
            <format>op $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Deop</name>
            <format>deop $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>GTFO + Reason</name>
            <format>gtfo $TARGET_NAME $REASON</format>
        </playerCommand>
        <playerCommand>
            <name>GTFO</name>
            <format>gtfo $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Toggle Freeze</name>
            <format>fr $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Toggle God</name>
            <format>god $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Cage</name>
            <format>cage $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Uncage</name>
            <format>uncage $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Doom</name>
            <format>doom $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Creative</name>
            <format>creative $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Survival</name>
            <format>survival $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Adventure</name>
            <format>adventure $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Seen Name</name>
            <format>seen $TARGET_NAME</format>
        </playerCommand>
        <playerCommand>
            <name>Seen IP</name>
            <format>seen $TARGET_IP</format>
        </playerCommand>
        <playerCommand>
            <name>Clear Nickname</name>
            <format>nick $TARGET_NAME off</format>
        </playerCommand>
    </playerCommands>
    <favoriteButtons>
        <favoriteButton>
            <command>opall</command>
            <label>Op All</label>
        </favoriteButton>
        <favoriteButton>
            <command>deopall</command>
            <label>Deop All</label>
        </favoriteButton>
        <favoriteButton>
            <command>staffmode on</command>
            <label>Staffmode ON</label>
        </favoriteButton>
        <favoriteButton>
            <command>staffmode off</command>
            <label>Staffmode OFF</label>
        </favoriteButton>
        <favoriteButton>
            <command>cake</command>
            <label>Cake</label>
        </favoriteButton>
        <favoriteButton>
            <command>cookie</command>
            <label>Cookie</label>
        </favoriteButton>
        <favoriteButton>
            <command>uall</command>
            <label>Undisguise All</label>
        </favoriteButton>
        <favoriteButton>
            <command>nickclean</command>
            <label>Nick Clean</label>
        </favoriteButton>
        <favoriteButton>
            <command>mp</command>
            <label>Mob Purge</label>
        </favoriteButton>
        <favoriteButton>
            <command>rd</command>
            <label>Remove Drops</label>
        </favoriteButton>
        <favoriteButton>
            <command>purgeall</command>
            <label>Purge All</label>
        </favoriteButton>
        <favoriteButton>
            <command>setl</command>
            <label>Set Limit = 2500</label>
        </favoriteButton>
        <favoriteButton>
            <command>fr</command>
            <label>Freeze All</label>
        </favoriteButton>
        <favoriteButton>
            <command>fr purge</command>
            <label>Unfreeze All</label>
        </favoriteButton>
        <favoriteButton>
            <command>blockcmd -a</command>
            <label>Block commands</label>
        </favoriteButton>
        <favoriteButton>
            <command>blockcmd purge</command>
            <label>Unblock commands</label>
        </favoriteButton>
        <favoriteButton>
            <command>mute all</command>
            <label>Mute all</label>
        </favoriteButton>
        <favoriteButton>
            <command>mute purge</command>
            <label>Unmute All</label>
        </favoriteButton>
        <favoriteButton>
            <command>cage purge</command>
            <label>Uncage all</label>
        </favoriteButton>
        <favoriteButton>
            <command>invis clear</command>
            <label>Clear Invis</label>
        </favoriteButton>
        <favoriteButton>
            <command>invis</command>
            <label>Invis List</label>
        </favoriteButton>
        <favoriteButton>
            <command>stop</command>
            <label>Stop Server</label>
        </favoriteButton>
    </favoriteButtons>
    <theme>
        <lastSelectedTheme>Light</lastSelectedTheme>
        <customThemePath/>
        <useCustomTheme>false</useCustomTheme>
        <darkTheme>false</darkTheme>
        <customThemeDarkTheme>false</customThemeDarkTheme>
        <lastSelectedFontSize>12</lastSelectedFontSize>
        <lastSelectedFont>Courier New</lastSelectedFont>
    </theme>
    <filters>
        <ignorePreprocessCommands>false</ignorePreprocessCommands>
        <ignoreServerCommands>false</ignoreServerCommands>
        <showChatOnly>false</showChatOnly>
        <ignoreWarnings>false</ignoreWarnings>
        <ignoreErrors>false</ignoreErrors>
        <showStaffChatOnly>false</showStaffChatOnly>
        <ignoreAsyncWorldEdit>false</ignoreAsyncWorldEdit>
        <ignoreGuildChat>false</ignoreGuildChat>
    </filters>
</configuration>